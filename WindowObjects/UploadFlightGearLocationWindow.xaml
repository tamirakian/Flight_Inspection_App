<Window x:Class="Flight_Inspection_App.WindowObjects.UploadFlightGearLocationWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Flight_Inspection_App.WindowObjects"
        mc:Ignorable="d"
        Title="Upload Flight GearLocation" Height="450" Width="800">
    <Grid HorizontalAlignment="Left" Height="335" Margin="232,52,0,0" VerticalAlignment="Top" Width="322" Background="White">
        <Button x:Name="UploadBtn"  Content="Upload File" HorizontalAlignment="Left" Margin="102,40,0,0" VerticalAlignment="Top" Width="75" Click="BtnUpload_Click"/>
        <TextBox x:Name="FilePath" HorizontalAlignment="Left" Height="20" Margin="19,76,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="245"/>
        <Button x:Name="SubmitBtn" Content="Submit" HorizontalAlignment="Left" Margin="102,198,0,0" VerticalAlignment="Top" Width="75" Click="BtnSubmit_Click"/>
    </Grid>
</Window>
